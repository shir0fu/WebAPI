    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>931c49f8-3c82-4093-bce0-c2e9a29c42fd</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="11.0.1" />
